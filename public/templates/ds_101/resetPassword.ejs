        <div id="container" class="container">  
            <div class="register-outer-form-wrapper">
                <div class="register-title">Reset Password</div>
                <div class="register-form-wrapper">
                    <form>                        
                        <div class="register-label" >
                            <label class="support-form-label" for="username">Username:</label>
                        </div>
                        <div class="register-element">
                            <input class="form-left-text-element" type="text" name="username"/>                          
                        </div>
                                                
                        <div class="register-label" >
                            <label class="support-form-label" for="email">Email</label>
                        </div>
                        <div class="register-element">
                            <input class="form-left-text-element" type="email" name="email"/>                          
                        </div>
                        
                        <div class="register-label" >
                            <label class="support-form-label" for="question">Question</label>
                        </div>
                        <div class="register-element">
                            <input class="form-left-text-element" type="text" name="question"/>                          
                        </div>
                        
                        <div class="register-label" >
                            <label class="support-form-label" for="answer">Answer</label>
                        </div>
                        <div class="register-element">
                            <input class="form-left-text-element" type="text" name="answer"/>  
                            <input id="key" type="hidden" name="key" /> 
                        </div>

                        <div class="register-button">
                            <button type="submit" class="">Register</button>
                        </div>                       
                    </form>                      
                </div>                
